https://leetcode.com/problems/rotate-array


class Solution {
    private void reverse(int[] nums, int start, int end){
        int i = 0;
        int len =(end-start)/2; 
        while(i<=len){
            int temp = nums[start];
            nums[start] = nums[end];
            nums[end] = temp;
            start++;
            end--;
            i++;
        }

    }
    
    
    public void rotate(int[] nums, int k) {
        int n = nums.length;
        k=k%n;
        if(n==1 || k==0){
            return;
        }

        reverse(nums, 0, n-k-1);
        reverse(nums, n-k,n-1);
        reverse(nums,0,n-1);
    }
}
