https://leetcode.com/problems/remove-duplicates-from-sorted-array/


class Solution {
    public int removeDuplicates(int[] nums) {
        int n = nums.length;
        if(n<2){
            return n;
        }
        int currIndex = 1;
        for(int i=1;i<n;i++){
            if(nums[i]!=nums[i-1]){
                nums[currIndex++] = nums[i];
            }
        }
        return currIndex;
    }
}
